static void run_test(UnityTestFunction func, const char *name, UNITY_LINE_TYPE line_num) {
  Unity.CurrentTestName = name;
  Unity.CurrentTestLineNumber = line_num;

  Unity.NumberOfTests++;
  UNITY_CLR_DETAILS();

  if (TEST_PROTECT()) {
{%- if data.has_in_task_test %}
    vTaskSuspendAll();
    setup_test();
    xTaskResumeAll();
{% else %}
    setup_test();
{%- endif %}
    func();
  }

  UnityConcludeTest();
}


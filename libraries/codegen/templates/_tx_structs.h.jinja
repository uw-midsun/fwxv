#pragma once

{%- set messages = data["Messages"] -%}
{%- set board = data["Board"] %}

#include <stdint.h>

typedef struct
{
{%- for message in messages -%}
    {%- for signal, signal_data in messages[message]['signals'].items() %}
    uint{{signal_data['length']}}_t {{message}}_{{signal}};
    {%- endfor -%}
{% endfor %}
} {{board}}_tx_struct;